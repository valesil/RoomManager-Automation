<suite name="ConferenceRoomOutOfOrderSuite">
 	<listeners>
      	<listener class-name="org.uncommons.reportng.HTMLReporter"/>
      	<listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
      	<listener class-name="framework.utils.ReportNGListener"/>
  	</listeners>
 	<test name = "ConferenceRoomOutOfOrderTests">
		<classes>
 			<class name="tests.admin.conferenceroomoutoforderplanning.StartTestConferenceRoomOutOfOrder" />
 			 	<!--TC01, TC28-->
 			 	<class name="tests.admin.conferenceroomoutoforderplanning.OutOfOrderIsCreatedAndEnabled"/>
